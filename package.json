{
  "name": "@qodunpob/component-demo-ui",
  "version": "0.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "lib:build": "npm run lib:clean && npm run lib:compile && npm run lib:copy-assets",
    "lib:clean": "rimraf dist",
    "lib:compile": "tsc --project tsconfig.build.json",
    "lib:copy-assets": "copyfiles -u 1 src/**/*.{css,svg} dist",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier . --write",
    "demo:clean": "rimraf demo/public/assets",
    "demo:run": "tsx demo.build.ts",
    "demo:open": "open-cli http://127.0.0.1:8000/",
    "start": "npm run demo:clean && concurrently \"npm run demo:run\" \"npm run demo:open\"",
    "prepare": "husky"
  },
  "lint-staged": {
    ".*{ts,tsx}": [
      "eslint"
    ],
    "*.{ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "classnames": "^2.5.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "classnames": "^2.5.1",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.19.12",
    "esbuild-plugin-svgr": "^2.1.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.1",
    "open-cli": "^8.0.0",
    "prettier": "^3.2.4",
    "prettier-config-standard": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "react-inspector": "^6.0.2"
  }
}
